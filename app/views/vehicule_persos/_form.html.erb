<%= simple_form_for(@vehicule_perso) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numMineralogique %>
    <%= f.input :puissanceVehicule %>
    <%= f.input :DateAutorisationAnnuelleDUtilisation %>
    <%= f.input :departement %>
    <%= f.input :academie %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
