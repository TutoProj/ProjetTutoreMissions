<%= simple_form_for(@vehicule_perso) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numMineralogique ,:label=>"Numero de la plaque mineralogique"%>
    <%= f.input :puissanceVehicule ,:label=>"Puissance du vehicule"%>
    <%= f.input :DateAutorisationAnnuelleDUtilisation ,:label=>"Date de l'autorisation annuelle d'autorisation"%>
    <%= f.input :departement ,:label=>"Departement"%>
    <%= f.input :academie ,:label=>"Academie"%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
