<%= simple_form_for(@vehicule_perso) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :numMineralogique ,:label=>"Numéro de la plaque minéralogique"%>
    <%= f.input :puissanceVehicule ,:label=>"Puissance du véhicule"%>
    <%= f.input :DateAutorisationAnnuelleDUtilisation ,:label=>"Date de l'autorisation annuelle d'autorisation"%>
    <%= f.input :departement ,:label=>"Département"%>
    <%= f.input :academie ,:label=>"Académie"%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
