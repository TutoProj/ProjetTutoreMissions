    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @moyen_transports.each do |moyen_transport| %>
  <tr>
    <td><%= moyen_transport.avion %></td>
    <td><%= moyen_transport.parkingAeroport %></td>
    <td><%= moyen_transport.train %></td>
    <td><%= moyen_transport.classe %></td>
    <td><%= moyen_transport.parkingGare %></td>
    <td><%= moyen_transport.reservation %></td>
    <td><%= moyen_transport.couchette %></td>
    <td><%= moyen_transport.wagonLit %></td>
    <td><%= moyen_transport.supplements %></td>
    <td><%= moyen_transport.taxi %></td>
    <td><%= moyen_transport.vehiculeLoue %></td>
    <td><%= moyen_transport.metro %></td>
    <td><%= moyen_transport.rer %></td>
    <td><%= moyen_transport.autobus %></td>
    <td><%= moyen_transport.tramway %></td>
    <td><%= moyen_transport.missionAvecTransport %></td>
    <td><%= moyen_transport.vehiculeAdmin %></td>
    <td><%= moyen_transport.fraisAutoroute %></td>
    <td><%= moyen_transport.parking %></td>
    <td><%= moyen_transport.vehiculePerso %></td>
    <td><%= moyen_transport.demandeAutorisationPonctuelleVehicule %></td>
    <td><%= link_to 'Afficher', moyen_transport %></td>
    <td><%= link_to 'Modifier', edit_moyen_transport_path(moyen_transport) %></td>
   
    <td><%= link_to 'Supprimer', moyen_transport, :method => :delete, :data => { :confirm => 'Etes-vous sur?' } %></td>

  </tr>
<% end %>
</table>

