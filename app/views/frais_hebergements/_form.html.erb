<%= simple_form_for(@frais_hebergement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :nbNuiteeAvecFrais %>
    <%= f.input :nbJustificatifs %>
    <%= f.input :montant %>
    <%= f.input :rembourse %>
    <%= f.input :logeDansAutreLogement %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
