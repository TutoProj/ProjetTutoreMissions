<%= simple_form_for(@frais_deplacement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :demandeAvance label:"Demande d'avance"%>
    <%= f.input :regularisationDAvance label:"Régularisation d'avance" %>
    <%= f.input :forfait label:"Forfait" %>
    <%= f.input :montant label:"Montant" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
