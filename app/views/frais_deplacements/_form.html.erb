<%= simple_form_for(@frais_deplacement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :demandeAvance %>
    <%= f.input :regularisationDAvance %>
    <%= f.input :forfait %>
    <%= f.input :montant %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
